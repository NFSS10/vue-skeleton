{
    "name": "vue-skeleton",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "prettier-check": "prettier --check \"**/*.+(vue|ts|js|tsx|jsx|json|css|md)\"",
        "prettier": "prettier --write \"**/*.+(vue|ts|js|tsx|jsx|json|css|md)\"",
        "pretty": "npm run prettier && npm run lint-fix",
        "type-check": "vue-tsc --noEmit",
        "dev": "vite",
        "build": "npm run type-check && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "vue": "^3.3.4",
        "vue-router": "^4.2.2"
    },
    "devDependencies": {
        "@types/node": "^20.3.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-typescript": "^11.0.3",
        "eslint": "^8.43.0",
        "eslint-plugin-vue": "^9.15.1",
        "prettier": "^2.8.8",
        "typescript": "^5.1.3",
        "vite": "^4.3.9",
        "vue-tsc": "^1.8.1"
    }
}
