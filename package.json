{
    "name": "vue-skeleton",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "prettier-check": "prettier --check \"**/*.+(vue|ts|js|tsx|jsx|json|css|md)\"",
        "prettier": "prettier --write \"**/*.+(vue|ts|js|tsx|jsx|json|css|md)\"",
        "pretty": "npm run prettier && npm run lint-fix",
        "type-check": "vue-tsc --noEmit",
        "dev": "vite",
        "build": "npm run type-check && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "eslint": "^8.38.0",
        "eslint-plugin-vue": "^9.10.0",
        "prettier": "^2.8.7",
        "typescript": "^5.0.4",
        "vite": "^4.2.1",
        "vue-tsc": "^1.2.0"
    }
}
