{
    "name": "vue-skeleton",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint .",
        "lint-fix": "eslint . --fix",
        "prettier-check": "prettier --check \"**/*.+(vue|ts|js|tsx|jsx|json|css|md)\"",
        "prettier": "prettier --write \"**/*.+(vue|ts|js|tsx|jsx|json|css|md)\"",
        "pretty": "npm run prettier && npm run lint-fix",
        "type-check": "vue-tsc --noEmit",
        "dev": "vite",
        "build": "npm run type-check && vite build",
        "preview": "npm run build && vite preview"
    },
    "dependencies": {
        "vue": "^3.3.11",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@types/node": "^20.10.4",
        "@vitejs/plugin-vue": "^4.5.2",
        "@vue/eslint-config-typescript": "^12.0.0",
        "eslint": "^8.55.0",
        "eslint-plugin-vue": "^9.19.2",
        "prettier": "^3.1.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.7",
        "vue-tsc": "^1.8.25"
    }
}
